<Window x:Class="PracticaCanvas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaCanvas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button>Boton Uno</Button>
        <Button Canvas.Right="0" Canvas.Top="0">Boton Dos</Button>
        <Button Canvas.Bottom="0" Canvas.Left="0">Boton Tres</Button>
        <Button Canvas.Bottom="0" Canvas.Right="0">Boton Cuatro</Button>
        <Rectangle Fill="Coral" Height="50" Width="50" Canvas.Top="100" Canvas.Left="100" Panel.ZIndex="4"></Rectangle>
        <Rectangle Fill="Black" Height="50" Width="50" Canvas.Top="120" Canvas.Left="120" Panel.ZIndex="2"></Rectangle>
    </Canvas>
</Window>
